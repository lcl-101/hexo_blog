<!-- Friends Blog -->
<section class="hidden-xs hidden-sm visible-md visible-lg">
<% if(config.friends) { %>
<h5>FRIENDS</h5>
<ul class="list-inline">

    <% config.friends.forEach(function(friend){ %>
        <li><a href="<%= friend.href %>" target="_blank"><%= friend.title %></a></li>
    <% }); %>
</ul>
<% }%>
</section>
